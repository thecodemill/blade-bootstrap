    "description": "A collection of Laravel Blade templates for Bootstrap integration.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Robinson",
            "email": "andrew@thecodemill.co.uk"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": ">=6.2"
    },
    "autoload": {
        "psr-4": {
            "TheCodeMill\\BladeBootstrap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TheCodeMill\\BladeBootstrap\\ServiceProvider"
            ]
        }
    }
}
